{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "communist-web-shop-app": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/communist-web-shop-app",
      "sourceRoot": "projects/communist-web-shop-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/communist-web-shop-app",
            "index": "projects/communist-web-shop-app/src/index.html",
            "browser": "projects/communist-web-shop-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/communist-web-shop-app/tsconfig.app.json",
            "assets": [
              "projects/communist-web-shop-app/src/favicon.ico",
              "projects/communist-web-shop-app/src/assets"
            ],
            "styles": [
              "projects/communist-web-shop-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "communist-web-shop-app:build:production"
            },
            "development": {
              "buildTarget": "communist-web-shop-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "communist-web-shop-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/communist-web-shop-app/tsconfig.spec.json",
            "assets": [
              "projects/communist-web-shop-app/src/favicon.ico",
              "projects/communist-web-shop-app/src/assets"
            ],
            "styles": [
              "projects/communist-web-shop-app/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "log4ts": {
      "projectType": "library",
      "root": "projects/log4ts",
      "sourceRoot": "projects/log4ts/src",
      "prefix": "lib",
      "architect": {
